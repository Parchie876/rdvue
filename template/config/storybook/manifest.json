{
  "version": 1,
  "name": "storybook",
  "description": "Generate a StoryBook preview.",
  "sourceDirectory": "./",
  "singleUsePerProject": true,
    
  "installDirectory": ".storyboard",
  "files": [
    {
      "source": "components/example.vue",
      "target": "example.vue"
    },
    {
      "source": "components/index.ts",
      "target": "index.ts"
    }, 
    {
      "source": "components/playground.vue",
      "target": "playground.vue"
    }, 
    {
      "source": "components/preview.vue",
      "target": "preview.vue"
    },  
    {
      "source": "modules/constants.ts",
      "target": "constants.ts"
    },
    {
      "source": "modules/index.ts",
      "target": "index.ts"
    },
    {
      "source": "modules/preview-of.ts",
      "target": "preview-of.ts"
    },
    {
      "source": "addons.js",
      "target": "addons.js"
    },
    {
      "source": "config.js",
      "target": "config.js"
    },
    {
      "source": "logo.svg",
      "target": "logo.svg"
    },
    {
      "source": "preview-head.html",
      "target": "preview-head.html"
      
    }, 
    {
      "source": "stories.scss",
      "target": "stories.scss"
      
    },
    {
      "source": "tailwind.css",
      "target": "webpack.config.js"
      
    }
  ]
}