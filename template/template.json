{
  "version": 1,
  "sourceDirectory": "./",
  "import": {
    "required": [
      "config",
      "store"
    ],
    "optional": [
      "component",
      "service",
      "model",
      "page",
      "auth",
      "sm",
      "localization"
    ],
    "groups": [
      {
        "isRequired": true,
        "name": "ui-library",
        "promptType": "list",
        "modules": [
          "bootstrap"
        ],
        "question": [
          "Which UI Library would like like to include?"
        ]
      },
      {
        "isRequired": false,
        "name": "auth",
        "promptType": "checkbox",
        "modules": [
          "auth0",
          "auth"
        ],
        "question": [
          "Which Authentication would you like to include?"
        ]
      }
    ],
    "presets": [
      {
        "name": "default",
        "dependencies": [
          "auth",
          "bootstrap"
        ]
      },
      {
        "name": "preset1",
        "dependencies": [
          "auth0"
        ]
      }
    ],
    "customPreset": {
      "name": "custom",
      "groups": [
        "ui-library",
        "auth"
      ]
    }
  }
}