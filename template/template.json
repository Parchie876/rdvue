{
  "version": 1,
  "sourceDirectory": "./",
  "features": [
    {
      "name": "config",
      "private": true
    },
    {
      "name": "store",
      "private": true
    },
    {
      "name": "component",
      "private": false
    },
    {
      "name": "service",
      "private": false
    },
    {
      "name": "model",
      "private": false
    },
    {
      "name": "page",
      "private": false
    },
    {
      "name": "sm",
      "private": false
    },
    {
      "name": "layout",
      "private": false
    }
  ],
  "plugins": [
    "localization",
    "storybook",
    "vuetify",
    "buefy"
  ],
  "project": {
    "features": [
      "config",
      "store"
    ],
    "optional": [
      "component",
      "service",
      "model",
      "page",
      "sm",
      "layout"
    ],
    "plugins": [
      "storybook"
    ]
   },
  "groups": [
    {
      "name": "auth",
      "isMultipleChoice": false,
      "plugins": [
        "auth"
      ],
      "question": "(INCOMPLETE. DO NOT USE) Which Authentication Library would you like to install?"
    },
    {
      "name": "locale",
      "isMultipleChoice": false,
      "plugins": [
        "localization"
      ],
      "question": "Which Localization Library would you like to install?"
    },
    {
      "name": "ui",
      "isMultipleChoice": false,
      "plugins": [
        "vuetify",
        "buefy"
      ],
      "question": "Which UI Library would you like to install?"
    }
  ],
  "presets": [
    {
      "name": "Recommended",
      "plugins": [
        "buefy",
        "localization",
        "fontawesome"
      ],
      "description": "Buefy, Localization, FontAwesome"
    },
    {
      "name": "Vuetify",
      "plugins": [
        "vuetify",
        "localization",
        "fontawesome"
      ],
      "description": "Vuetify, Localization, FontAwesome"
    }
  ],
  "customPreset": {
    "groups": ["ui"],
    "name": "custom"
  }
}